# OnchainKit API Key (get from https://portal.cdp.coinbase.com)
VITE_PUBLIC_ONCHAINKIT_API_KEY=your_api_key_here

# WebSocket backend URL
VITE_BACKEND_URL=ws://localhost:3001/ws

# Smart contract address
VITE_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000

# Network: testnet or mainnet
VITE_NETWORK=testnet

# WalletConnect Project ID (optional, for non-MiniKit contexts)
VITE_WALLET_CONNECT_PROJECT_ID=your_project_id
